@model IEnumerable<ShipperModel>

<h1>Shipper List</h1>


<table class="table">
    <thead class="thead-light">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Phone</th>
        </tr>
    </thead>
    <tbody>

        @{

            foreach (var m in Model)
            {
                <tr>
                    <td>@m.Id</td>
                    <td>@m.Name</td>

                    <td>@m.Phone</td>

                </tr>
            }
        }


    </tbody>
</table>


<a asp-action="Index" asp-controller="Shipper" class="bg-success btn">Back to Shipper Page</a>

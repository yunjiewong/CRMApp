@model IEnumerable<ProductResponseModel>

<h1>Product List</h1>


<table class="table">
    <thead class="thead-light">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">CategoryId</th>
            <th scope="col">Discontinued</th>

            <th scope="col">UnitsInStock</th>
            <th scope="col">UnitsOnOrder</th>
            <th scope="col">UnitPrice</th>
            <th scope="col">QuantityPerUnit</th>
        </tr>
    </thead>
    <tbody>

        @{

            foreach (var m in Model)
            {
                <tr>
                    <td>@m.Id</td>
                    <td>@m.Name</td>
                    <td>@m.CategoryId</td>
                    <td>@m.Discontinued</td>

                    <td>@m.UnitsInStock</td>
                    <td>@m.UnitsOnOrder</td>
                    <td>@m.UnitPrice</td>
                    <td>@m.QuantityPerUnit</td>
                </tr>
            }
        }


    </tbody>
</table>


<a asp-action="Index" asp-controller="Shipper" class="bg-success btn">Back to Shipper Page</a>
